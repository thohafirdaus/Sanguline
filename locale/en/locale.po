msgid "plugins.themes.sanguline.name"
msgstr "Sanguline Theme"

msgid "plugins.themes.sanguline.description"
msgstr "A modern and elegant theme for OJS. Created by Thoha Firdaus with love for the academic community."

msgid "plugins.themes.sanguline.option.typography.label"
msgstr "Typography"

msgid "plugins.themes.sanguline.option.typography.description"
msgstr "Choose a font combination that suits this journal."

msgid "plugins.themes.sanguline.option.typography.notoSans"
msgstr ""
"Noto Sans: A digital-native font designed by Google for extensive language "
"support."

msgid "plugins.themes.sanguline.option.typography.notoSerif"
msgstr "Noto Serif: A serif variant of Google's digital-native font."

msgid "plugins.themes.sanguline.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: A complementary pairing with serif headings and sans-"
"serif body text."

msgid "plugins.themes.sanguline.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: A complementary pairing with sans-serif headings and "
"serif body text."

msgid "plugins.themes.sanguline.option.typography.lato"
msgstr "Lato: A popular modern sans-serif font."

msgid "plugins.themes.sanguline.option.typography.lora"
msgstr "Lora: A wide-set serif font good for reading online."

msgid "plugins.themes.sanguline.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: A complimentary pairing with serif headings and sans-serif "
"body text."

msgid "plugins.themes.sanguline.option.colour.label"
msgstr "Primary Colour"

msgid "plugins.themes.sanguline.option.colour.description"
msgstr "Choose a primary colour for the theme. This colour will be applied to the header, borders, links, and accent elements throughout the site."

msgid "plugins.themes.sanguline.option.showDescriptionInJournalIndex.option"
msgstr "Show the journal summary on the homepage."

msgid "plugins.themes.sanguline.option.useHomepageImageAsHeader.label"
msgstr "Move Homepage Image to Header"

msgid "plugins.themes.sanguline.option.useHomepageImageAsHeader.description"
msgstr ""
"If enabled, the homepage image will be moved to the header background and "
"will NOT be displayed in its usual position on the homepage. Leave unchecked "
"to show the image in the homepage hero section."

msgid "plugins.themes.sanguline.option.useHomepageImageAsHeader.option"
msgstr "Move homepage image to header background (hides from homepage hero)."

msgid "plugins.themes.sanguline.option.displayStats.label"
msgstr "Usage statistics display options"

msgid "plugins.themes.sanguline.option.displayStats.none"
msgstr "Do not display submission usage statistics chart for reader."

msgid "plugins.themes.sanguline.option.displayStats.bar"
msgstr "Use bar type of the chart for usage statistics display."

msgid "plugins.themes.sanguline.option.displayStats.line"
msgstr "Use line type of the chart for usage statistics display."

msgid "plugins.themes.sanguline.displayStats.monthInitials"
msgstr "Jan Feb Mar Apr May Jun Jul Aug Sep Oct Nov Dec"

msgid "plugins.themes.sanguline.displayStats.downloads"
msgstr "Downloads"

msgid "plugins.themes.sanguline.displayStats.noStats"
msgstr "Download data is not yet available."

msgid "plugins.themes.sanguline.navigation.sidebar"
msgstr "Sidebar"

msgid "plugins.themes.sanguline.sidebar.additionalMenu"
msgstr "Additional Menu"

msgid "plugins.themes.sanguline.option.homepageImageWidth.label"
msgstr "Homepage Image Width"

msgid "plugins.themes.sanguline.option.homepageImageWidth.description"
msgstr "Set the width of the homepage image when displayed side by side with the journal description."

msgid "plugins.themes.sanguline.option.homepageImageWidth.small"
msgstr "Small (30%)"

msgid "plugins.themes.sanguline.option.homepageImageWidth.medium"
msgstr "Medium (40%)"

msgid "plugins.themes.sanguline.option.homepageImageWidth.large"
msgstr "Large (50%)"

msgid "plugins.themes.sanguline.option.sidebarNavDisplay.label"
msgstr "Sidebar Navigation Display"

msgid "plugins.themes.sanguline.option.sidebarNavDisplay.description"
msgstr "Choose how to display menu items in the sidebar navigation."

msgid "plugins.themes.sanguline.option.sidebarNavDisplay.inline"
msgstr "Inline - Menu items displayed side by side"

msgid "plugins.themes.sanguline.option.sidebarNavDisplay.block"
msgstr "Block - Menu items stacked vertically"

msgid "plugins.themes.sanguline.option.headerLogoSize.label"
msgstr "Header Logo Size"

msgid "plugins.themes.sanguline.option.headerLogoSize.description"
msgstr "Choose the size of the logo/site name in the header."

msgid "plugins.themes.sanguline.option.headerLogoSize.small"
msgstr "Small (60px)"

msgid "plugins.themes.sanguline.option.headerLogoSize.medium"
msgstr "Medium (80px)"

msgid "plugins.themes.sanguline.option.headerLogoSize.large"
msgstr "Large (100px)"

msgid "plugins.themes.sanguline.option.headerLogoSize.xlarge"
msgstr "Extra Large (120px)"

msgid "plugins.themes.sanguline.navigation.home"
msgstr "Home"

msgid "plugins.themes.sanguline.article.views"
msgstr "Views"

msgid "plugins.themes.sanguline.article.downloads"
msgstr "Downloads"

msgid "plugins.themes.sanguline.author.affiliation"
msgstr "Affiliation"

msgid "plugins.themes.sanguline.author.country"
msgstr "Country"

msgid "plugins.themes.sanguline.article.showAbstract"
msgstr "Show Abstract"

msgid "plugins.themes.sanguline.article.pages"
msgstr "Pages"

